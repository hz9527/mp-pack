{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "no-plusplus": 0,
    "no-prototype-builtins": 0,
    "no-unused-expressions": [2, {
      "allowShortCircuit": true,
      "allowTernary": true
    }],
    "no-console": 0,
    "import/no-unresolved": 0,
    "no-void": 0
  },
  "globals": {
    "Page": true,
    "Component": true,
    "App": true,
    "tt": true,
    "getCurrentPages": true
  }
}